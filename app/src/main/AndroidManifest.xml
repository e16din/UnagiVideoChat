<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unagimagi">

    <application
        android:name=".screens.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".screens.roulettechat.RouletteChatActivity"
            android:screenOrientation="unspecified"/>
        <activity android:name=".screens.room.RoomActivity"
            android:screenOrientation="unspecified"/>
        <activity android:name=".screens.profile.ProfileActivity"
            android:screenOrientation="unspecified"/>
        <activity android:name=".screens.rooms.RoomsActivity"
            android:screenOrientation="unspecified"/>
        <activity android:name=".screens.login.LoginActivity"
            android:screenOrientation="unspecified">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>